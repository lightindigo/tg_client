{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es2017",
        "esModuleInterop": false,
        "allowSyntheticDefaultImports": true,
        "noEmitOnError": true,
        "resolveJsonModule": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitUseStrict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true
    }
}